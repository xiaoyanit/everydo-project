<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="Anonymous"/>
    <role name="Authenticated"/>
    <role name="Manager"/>
    <role name="Administrator"/>
    <role name="Member"/>
    <role name="Owner"/>
    <role name="Reviewer"/>
    <role name="Reader" />
    <role name="Editor" />
    <role name="Responsor" />
  </roles>
  <permissions>
    <permission name="Add portal content" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Contributor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Add portal folders" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Contributor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="ATContentTypes: Add Document" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Contributor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="ATContentTypes: Add File" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Contributor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="ATContentTypes: Add Folder" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Contributor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="ATContentTypes: Add Image" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Contributor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="ATContentTypes: Add Large Plone Folder" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Contributor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Allow sendto" acquire="True">
      <role name="Anonymous"/>
      <role name="Manager"/>
      <role name="Member"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Delete objects" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Administrator"/>
    </permission>
    <permission name="List folder contents" acquire="True">
      <role name="Manager"/>
      <role name="Member"/>
      <role name="Owner"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Add portal member" acquire="False">
        <role name="Manager"/>
        <role name="Administrator"/>
    </permission>
    <permission name="Manage properties" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Editor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Modify view template" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Editor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Request review" acquire="True">
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Editor"/>
      <role name="Administrator"/>
      <role name="Reviewer"/>
    </permission>
    <permission name="List portal members" acquire="True">
      <role name="Manager"/>
      <role name="Member"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Manage properties" acquire="True">
      <role name="Manager"/>
      <role name="Editor"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Manage users" acquire="True">
      <role name="Manager"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Reply to item" acquire="True">
      <role name="Manager"/>
      <role name="Reader" />
      <role name="Editor" />
      <role name="Member"/>
      <role name="Administrator"/>
    </permission>
    <permission name="Review portal content" acquire="True">
      <role name="Manager"/>
      <role name="Reviewer"/>
      <role name="Administrator"/>
    </permission>

    <permission name="Portlets: Manage own portlets"
                acquire="True">
      <role name="Manager"/>
      <role name="Member"/>
      <role name="Administrator"/>
    </permission>
    <permission name="View"
                acquire="True">
        <role name="Reader" />
        <role name="Editor" />
        <role name="Contributor" />
      <role name="Administrator"/>
    </permission>
    <permission name="Access contents information"
                acquire="True">
        <role name="Reader" />
        <role name="Editor" />
        <role name="Contributor" />
      <role name="Administrator"/>
    </permission>
    <permission name="Modify portal content"
                acquire="True">
      <role name="Editor" />
      <role name="Administrator"/>
      <role name="Owner" />
      <role name="Manager" />
    </permission>
    <permission name="CMFEditions: Access previous versions"
        acquire="True">
        <role name="Editor"/>
        <role name="Member"/>
        <role name="Owner"/>
        <role name="Reader"/>
        <role name="Administrator"/>
    </permission>
    <permission name="CMFEditions: Purge version"
        acquire="True">
        <role name="Editor"/>
        <role name="Owner"/>
        <role name="Administrator"/>
    </permission>
  </permissions>
</rolemap>
